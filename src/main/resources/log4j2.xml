<Configuration status="ERROR" name="ExcelUtil">
    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} io.github.nambach.ExcelUtil [%p] %m%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Logger name="io.github.nambach.excelutil" level="trace" additivity="false">
            <AppenderRef ref="stdout"/>
        </Logger>
        <Root level="error">
            <AppenderRef ref="stdout"/>
        </Root>
    </Loggers>
</Configuration>

